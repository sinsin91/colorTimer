{
	"manifest_version": 2,
	"name": "ColorTimer",
	"version": "1.0",
	"version_name": "1.0 beta",
	"description": "to test new tab chrome extension",

	"icons": { 	"16": "images/icon16.png",
				"48": "images/icon48.png",
				"128": "images/icon128.png" 
	},

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [{
	    "matches": ["file:///*"],
	    "js": ["content.js"]
	  }],

	  "chrome_url_overrides" : {
	    "newtab": "tab.html"
	  },	

	"author": "yoonsin91"     
}